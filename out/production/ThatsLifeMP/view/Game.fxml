<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane>
         <content>
            <Canvas fx:id="board" height="200.0" width="200.0" />
         </content>
      </ScrollPane>
      <HBox spacing="20.0" style="-fx-background-color: #A9DFBF;">
         <children>
            <Label text="PLAYER">
               <font>
                  <Font name="3ds Regular" size="12.0" />
               </font>
            </Label>
            <Label text="CASH">
               <font>
                  <Font name="3ds Regular" size="12.0" />
               </font>
            </Label>
            <Label text="CAREER">
               <font>
                  <Font name="3ds Regular" size="12.0" />
               </font>
            </Label>
            <Label text="HOUSE">
               <font>
                  <Font name="3ds Regular" size="12.0" />
               </font>
            </Label>
            <Label text="SALARY">
               <font>
                  <Font name="3ds Regular" size="12.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
